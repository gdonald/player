
<div data-controller="mp3s" data-mp3s-url="<%= search_mp3s_path %>">
  <%= simple_form_for :search do |f| %>
    <%= f.input :query, label: 'Search', required: false, label: false, placeholder: 'Search term' %>
  <% end %>
  <div id="results">
    <%= render partial: 'list', locals: { mp3s: @mp3s } %>
  </div>
</div>
